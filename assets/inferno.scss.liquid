/*================ Collection Grids and Lists ================*/


.filters {
  list-style: none;
  margin: 0;

  .filter {
    padding: 0 0 15px 0;

    input {
      display: none;
    }

    label {
      display: block;
      position: relative;
      text-transform: capitalize;
      padding-left: 25px;
      @include transition(all ease-out 0.35s);

      &:hover {
        background: rgba($color-image-overlay,.1);
      }

      &:before {
        content: "";
        display: block;
        position: absolute;
        width: 16px;
        height: 16px;
        left: 0;
        top: 50%;
        margin-top: -8px;
        border: 1px solid $color-border;
        border-radius: 8px;
      }

      &:after {
        content: "";
        display: block;
        position: absolute;
        width: 0;
        height: 0;
        top: 50%;
        left: 8px;
        margin-top: 0;
        background: $color-border;
        border-radius: 4px;
        @include transition(all ease-out 0.35s);
      }
    }

    :checked + label {
      &:after {
        height: 8px;
        width: 8px;
        margin-top: -4px;
        left: 4px;
      }
    }

    :disabled + label {
      pointer-events: none;
      opacity: .5;
      color: lighten($color-text,10%);
