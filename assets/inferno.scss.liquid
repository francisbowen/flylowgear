.article-content {
  .article--gallery_column {
    position: relative;
    padding: 0 15px;
    &:before {
      content: '';
      position: absolute;
      top: 0;
      right: 0;
      bottom: 0;
      left: 0;
      background-color: #1d1934;
      opacity: 0.5;
      mix-blend-mode: multiply;
      pointer-events: none;
      z-index: 1;
    }
    &:after {
      content: "\f03e";
      font-family: FontAwesome;
      position: absolute;
      top: 50%;
      left: 50%;
      -ms-transform: translate(-50%, -50%);
      -webkit-transform: translate(-50%, -50%);
      transform: translate(-50%, -50%);
      font-size: 50px;
      border: 3px solid #fff;
      border-radius: 100%;
      width: 125px;
      height: 125px;
      text-align: center;
      line-height: 125px;
      color: #fff;
      pointer-events: none;
      z-index: 2;
      @include media-query($max,$lg){
        width: 70px;
        height: 70px;
        line-height: 65px;
        font-size: 35px;
      }
    }
    a {
      background-size: cover;
      background-repeat: no-repeat;
      background-position: center;
      &:before {
        content: '';
        display: block;
        padding-top: 100%;
      }
    }
  }
}
